#:kivy 2.3.0


<RandomDev>:
    orientation: "vertical"
    Label:
        text: "Random_Devotion_Time"
        text_size: root.width, None
        font_size: 150
        halign: 'center'
        size: self.texture_size
        size_hint_y: .2


    Button:
        text: "Get New Devotion"
        pos_hint: {'center': 1}
        background_color: (1, 0, 0, 1)